.page-header
  %h1
    Statistik
    %small= session[:date]


.well.col-md-6
  %h3
    Bisher verbraucht
  %table.table.table-bordered.table-condensed.table-hover
    %thead
      %tr
        %th Bier
        %th Wurst
        %th Schwein
        %th Pute
        %th Mais
  
    %tbody
      -# @dates.each do |date|
      %tr
        %td= @count_beer
        %td= @count_sausage
